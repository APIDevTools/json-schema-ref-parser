  "description": "Parse, Resolve, and Dereference JSON Schema $ref pointers",
  "keywords": [
    "json",
    "schema",
    "jsonschema",
    "json-schema",
    "json-pointer",
    "$ref",
    "dereference",
    "resolve"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "contributors": [
    {
      "name": "Boris Cherny",
      "email": "boris@performancejs.com"
    },
    {
      "name": "Phil Sturgeon",
      "email": "phil@apisyouwonthate.com"
    },
    {
      "name": "Jakub RoÅ¼ek",
      "email": "jakub@stoplight.io"
